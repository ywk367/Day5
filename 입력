import java.util.Scanner;

class Main {
  public static void main(String[] args) {

    
    
    Scanner scan = new Scanner(System.in);
    int n;
    float f;

    n = scan.nextInt();
    f = scan.nextFloat();
    System.out.println("input n = " + String.valueOf(n));
    System.out.println("input n = " + String.valueOf(f));
  }
}
